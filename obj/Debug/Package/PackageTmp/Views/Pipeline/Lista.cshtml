
@{
    ViewBag.Title = "Anexos";
    Layout = null;
    
}

@Styles.Render("~/Styles/Utils/bootstrap.css")
@Scripts.Render("~/Scripts/jquery-1.9.1.js")
@Scripts.Render("~/Scripts/jquery-1.9.1.min.js")
@Scripts.Render("~/Scripts/bootstrap.js")


<h2>Arquivos anexados ao projeto</h2>

<table align="center">

    @foreach (var arq in ViewBag.lista)
    {


        <tr>
            <td>
                @arq.nome
            </td>
            <td>
                <form name="form1" action="/Pipeline/Baixar?pasta=@ViewBag.pasta&dir=@arq.nome" method="post">
                    <input type="submit" class="input form-control btn btn-success active" value="Baixar"/>
                    </form>
                </td>
        </tr>

    }

</table>